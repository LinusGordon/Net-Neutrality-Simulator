{
  "manifest_version": 2,
  "name": "Net Neutrality Simulator",
  "description": "Simulate possible outcomes of the net neutrality repeal",
  "version": "1.0",

  "browser_action": {
   "default_popup": "popup.html"
  },

  "content_scripts": [
    { 
      "matches": [ "*://*/*" ],
      "js":[ "simulator.js" ]
    }],

   "background": {
    "scripts": ["background.js"]
  },
  "permissions": [ "activeTab", "storage", "webNavigation", "tabs"]
}